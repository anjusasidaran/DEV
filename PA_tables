
CREATE TABLE PA_DELIVERY (PA_DELIVERY_ID SERIAL PRIMARY KEY,PA_NAME VARCHAR(50),PA_MANAGER_NAME VARCHAR(25),PA_ACCOUNT_NAME VARCHAR(50),
PA_ACCOUNT_MANAGER_NAME VARCHAR(25),YEAR VARCHAR(10),MONTH VARCHAR(15),HC_GD VARCHAR(10),CURRENT_REV_TARGET VARCHAR(10),
REV_YTD_ACTUAL_IN_MILL VARCHAR(10),CURRENT_REV_EAC VARCHAR(10),CURRENT_GP_TARGET VARCHAR(10),CURRENT_GP_YTD VARCHAR(10),
CURRENT_EAC_GP VARCHAR(10),REV_TARGET_IN_MILL VARCHAR(10),REV_FORECAST_IN_MILL VARCHAR(10),REV_ACTUAL_IN_M VARCHAR(10),GP_PER_TARGET VARCHAR(10),
GP_PER_ACTUAL VARCHAR(10),EAC_GP VARCHAR(40),SIGNINGS VARCHAR(10),CSAT VARCHAR(10),CSAT_TREND_IND VARCHAR(15),LEAN_TARGET VARCHAR(10),LEAN_SAVINGS VARCHAR(15),
LEAN_SAVINGS_EAC VARCHAR(15),BAND_MIX VARCHAR(10),SOLUTIONED_BAND_MIX VARCHAR(10),ATTRITION VARCHAR(10),CHECK_POINT_COMP VARCHAR(10),
GH_PER VARCHAR(10),STAFFING_REVENUE_IMPACT VARCHAR(1000),COUNT_PROJ_ENHANCE_NEWDEV VARCHAR(1000),IDEA_BAG_INSIDE_OUT VARCHAR(10),
BAG_TCV_PLAND_USD VARCHAR(10),TCV_REALIZED_USD VARCHAR(10),VLI_IMPL_STATUS VARCHAR(15),CBKI_AUTO_LIME_SURVEY VARCHAR(1000),
KEY_ACCOUNT_CHALLENGE VARCHAR(1000),MITIGATION_ACTION_PLAN VARCHAR(1000),UPDATED_DATE DATE DEFAULT CURRENT_TIMESTAMP);


CREATE TABLE PA_TRENDS (TRENDS_ID SERIAL PRIMARY KEY,PA_NAME VARCHAR(50),MONTH VARCHAR(15),PREVIOUS_YEAR VARCHAR(20),CURRENT_YEAR VARCHAR(20),PREVIOUS_HC_TOTAL VARCHAR(20),
CURRENT_HC_TOTAL VARCHAR(20),PREVIOUS_YTD_REVENUE VARCHAR(20),CURRENT_YTD_REVENUE VARCHAR(20),PREVIOUS_YTD_GP_PER VARCHAR(20),CURRENT_YTD_GP_PER VARCHAR(20),PREVIOUS_SINGNINGS VARCHAR(20),
CURRENT_SIGNINGS VARCHAR(20),PREVIOUS_CSAT VARCHAR(20),CURRENT_CSAT VARCHAR(20),PREVIOUS_BAND_MIX VARCHAR(20),CURRENT_BAND_MIX VARCHAR(20),PREVIOUS_ATTRITION VARCHAR(20),
CURRENT_ATTRITION VARCHAR(20),PREVIOUS_GH_PER VARCHAR(20),CURRENT_GH_PER VARCHAR(20),UPDATED_DATE DATE DEFAULT CURRENT_TIMESTAMP); 
